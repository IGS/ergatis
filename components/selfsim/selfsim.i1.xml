<?xml version="1.0" encoding="UTF-8"?>

<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation='commandSet.xsd'>
    <commandSet type="serial">
        <name>Selfsim compute</name>
        <command>
            <type>RunUnixCommand</type>
            <name>selfsim</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/selfsim</executable>
			<arg>$;I_FILE_PATH$;</arg>
			<param>
				<key>stdout</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.txt</value>
			</param>	
        </command>
    </commandSet>
</commandSetRoot>
