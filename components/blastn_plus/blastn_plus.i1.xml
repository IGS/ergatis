<?xml version="1.0" encoding="UTF-8"?>

<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation='commandSet.xsd'>
    <commandSet type="serial">
        <name>blastn+ compute</name>
        <command>
            <type>RunUnixCommand</type>
            <name>run blastn+</name>
            <state>incomplete</state>
			<executable>$;BLASTPLUS_BIN$;/blastn</executable>
            <arg>-task $;ALGORITHM$; -query $;I_FILE_PATH$; -db $;DATABASE_PATH$; -evalue $;EXPECT$; -dust '$;FILTER$;' -num_alignments $;DATABASE_MATCHES$; -num_descriptions $;DESCRIPTIONS$; -outfmt 11 -out $;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.asn $;OTHER_OPTS$;</arg>
		</command>
		<!-- This command is used in the LGTSeek pipeline -->
        <command>
            <type>RunUnixCommand</type>
            <name>format asn to m8</name>
            <state>incomplete</state>
			<executable>$;BLASTPLUS_BIN$;/blast_formatter</executable>
            <arg>-archive $;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.asn -outfmt 6 </arg>
            <param>
				<key>stdout</key>
				<value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.m8</value>
			</param>  
        </command>
		<!-- Following stuff deals with things used in the Prokaryotic Annotation Pipeline -->
        <command>
            <type>RunUnixCommand</type>
            <name>format asn to pairwise</name>
            <state>incomplete</state>
			<executable>$;BLASTPLUS_BIN$;/blast_formatter</executable>
            <arg>-archive $;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.asn -outfmt 0 </arg>
            <param>
				<key>stdout</key>
				<value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.raw</value>
			</param>  
        </command>
        <command>
            <type>RunUnixCommand</type>
            <name>convert raw to btab</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/blast2btab</executable>
            <param>  
                <key>--input</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.raw</value>
            </param>
            <param>  
                <key>--output</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.btab</value>
	    </param>
	    <param>
	      <key>--split</key>
              <value>$;SPLIT_BTAB$;</value>
            </param>
        </command>
        <command>
            <type>RunUnixCommand</type>
            <name>convert raw to bsml</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/blast2bsml</executable>
            <param>
                <key>--query_file_path</key>
                <value>$;I_FILE_PATH$;</value>
            </param>
            <param>
                <key>--input</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.raw</value>
            </param>
            <param>
                <key>--output</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.bsml</value>
            </param>
            <param>
                <key>--pvalue</key>
                <value>$;EXPECT$;</value>
            </param>
            <param>
                <key>--filter_hsps_for_stats</key>
                <value>$;FILTER_HSPS_FOR_STATS$;</value>
            </param>
            <param>
                <key>--class</key>
                <value>polypeptide</value>
            </param>
            <param>
                <key>--analysis_id</key>
                <value>$;COMPONENT_NAME$;_analysis</value>
            </param>
	    <param>
                <key>--split</key>
                <value>$;SPLIT_BSML$;</value>
            </param>
            <param>
                <key>--gzip_output</key>
                <value>$;COMPRESS_BSML_OUTPUT$;</value>
            </param>
        </command>
        <command>
            <type>RunUnixCommand</type>
            <name>store config params</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/analysis2bsml</executable>
            <param>  
                <key>--ergatis_control_file</key>
                <value>$;ERGATIS_DIR$;.ini</value>
            </param>
            <param>  
                <key>--conf</key>
                <value>$;COMPONENT_CONFIG$;</value>
            </param>
            <param>  
                <key>--bsml_file</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.bsml</value>
            </param>
            <param>  
                <key>--componenturl</key>
                <value>$;WORKFLOW_REPOSITORY$;/component.xml</value>
            </param>
            <param>  
                <key>--pipelineurl</key>
                <value>$;REPOSITORY_ROOT$;/workflow/runtime/pipeline/$;PIPELINEID$;/pipeline.xml</value>
            </param>
            <param>  
                <key>--analysis_id</key>
                <value>$;COMPONENT_NAME$;_analysis</value>
            </param>
        </command>
        <command>
            <type>RunUnixCommand</type>
            <name>compress raw output</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/compress_file</executable>
            <param>  
                <key>--file</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.raw</value>
            </param>
            <param>  
                <key>--compress</key>
                <value>$;COMPRESS_RAW_OUTPUT$;</value>
            </param>
            <param>  
                <key>--list_file</key>
                <value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.raw.list</value>
            </param>
        </command>
        <command>
            <type>RunUnixCommand</type>
            <name>dtd validation</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/dtdValid.pl</executable>
            <param>  
                <key>-d</key>
                <value>$;DOCS_DIR$;/bsml3_1.dtd</value>
            </param>
            <arg>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.$;COMPONENT_NAME$;.bsml</arg>
        </command>
    </commandSet>
</commandSetRoot>
