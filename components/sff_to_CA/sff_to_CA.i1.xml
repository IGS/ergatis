<?xml version="1.0" encoding="UTF-8"?>

<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation='commandSet.xsd'>
    <commandSet type="serial">
        <name>sff_to_CA compute</name>
        <command>
            <type>RunUnixCommand</type>
            <state>incomplete</state>
            <name>create group output directory</name>
            <executable>mkdir</executable>
            <arg>-p -m 777 $;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;</arg>
        </command>
        <command>
            <type>RunUnixCommand</type>
            <state>incomplete</state>
            <name>create group temp directory</name>
            <executable>mkdir</executable>
            <arg>-p -m 777 $;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;</arg>
        </command>
        <command>
            <type>RunUnixCommand</type>
            <state>incomplete</state>
            <name>sffToCA</name>
            <executable>$;SFFTOCA_EXEC$;</executable>
            <arg>-libraryname $;LIBRARY_NAME$; -trim $;TRIM$; -output $;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.frg -log $;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.log -stats $;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.stats $;I_FILE_PATH$;</arg>
            <param>
                <key>stdout</key>
                <value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.stdout</value>
            </param>
            <param>
                <key>stderr</key>
                <value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.stderr</value>
            </param>
        </command>
    </commandSet>
</commandSetRoot>
