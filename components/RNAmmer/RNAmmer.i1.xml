<?xml version="1.0" encoding="UTF-8"?>

<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation='commandSet.xsd'>
    <commandSet type="serial">
        <name>RNAmmer compute</name>
        <command>
            <type>RunUnixCommand</type>
            <name>RNAmmer</name>
            <state>incomplete</state>
            <executable>$;RNAMMER_EXEC$;</executable>
            <arg>-S $;KINGDOM$; -m $;MOLECULES$; -xml $;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.xml -gff $;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.gff -h $;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.hmmreport -f $;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.fsa $;I_FILE_PATH$;</arg>
            <param>  
                <key>stderr</key>
                <value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.stderr</value>
            </param>
        </command>
    </commandSet>
</commandSetRoot>
