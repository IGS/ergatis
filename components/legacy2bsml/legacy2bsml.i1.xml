<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"			xsi:schemaLocation='commandSet.xsd'>
    <commandSet type="serial">
	<name>legacy2bsml</name>
	<command>
	    <type>RunUnixCommand</type>
	    <state>incomplete</state>
	    <name>Create the FASTA output directory</name>
	    <executable>mkdir</executable>
	    <arg>-p -m 777 $;FASTA_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/</arg>
	</command>
	<command>
	    <type>RunUnixCommand</type>
	    <state>incomplete</state>
	    <executable>$;BIN_DIR$;/legacy2bsml</executable>
	    <name>legacy2bsml</name>
	    <param>
		<key>--database</key>
		<value>$;SOURCE_DATABASE$;</value>
	    </param>
	    <param>
		<key>--password</key>
		<value>$;PASSWORD$;</value>
	    </param>
	    <param>
		<key>--username</key>
		<value>$;USERNAME$;</value>
	    </param>
	    <param>
		<key>--mode</key>
		<value>$;MODE$;</value>
	    </param>
	    <param>
		<key>--bsml_doc_name</key>
		<value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;SOURCE_DATABASE$;_$;ASMBL_ID$;_assembly.$;SCHEMA_TYPE$;.bsml</value>
	    </param>
	    <param>
		<key>--log4perl</key>
		<value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;SOURCE_DATABASE$;.$;ASMBL_ID$;.$;SCHEMA_TYPE$;.legacy2bsml.pl.log</value>
	    </param>
	    <param>
		<key>--debug_level</key>
		<value>$;DEBUG$;</value>
	    </param>
	    <param>
		<key>--fastadir</key>
		<value>$;FASTA_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/</value>
	    </param>
	    <param>
		<key>--asmbl_id</key>
		<value>$;ASMBL_ID$;</value>
	    </param>
	    <param>
		<key>--schema_type</key>
		<value>$;SCHEMA_TYPE$;</value>
	    </param>
	    <param>
		<key>--sequence_type</key>
		<value>$;SEQUENCE_TYPE$;</value>
	    </param>
	    <param>
		<key>--exclude_genefinders</key>
		<value>$;EXCLUDE_GENEFINDERS$;</value>
	    </param>
	    <param>
		<key>--include_genefinders</key>
		<value>$;INCLUDE_GENEFINDERS$;</value>
	    </param>
	    <param>
		<key>--alt_database</key>
		<value>$;ALT_DATABASE$;</value>
	    </param>
	    <param>
		<key>--alt_species</key>
		<value>$;ALT_SPECIES$;</value>
	    </param>
	    <param>
		<key>--id_repository</key>
		<value>$;PROJECT_ID_REPOSITORY$;</value>
	    </param>
	    <param>
		<key>--input_id_mapping_directories</key>
		<value>$;INPUT_ID_MAPPING_DIRECTORIES$;</value>
	    </param>
	    <param>
		<key>--input_id_mapping_files</key>
		<value>$;INPUT_ID_MAPPING_FILES$;</value>
	    </param>
	    <param>
		<key>--output_id_mapping_file</key>
		<value>$;TMP_DIR$;/$;DATABASE$;_$;ASMBL_ID$;_$;SCHEMA_TYPE$;.idmap</value>
	    </param>
	    <param>
		<key>--idgen_identifier_version</key>
		<value>$;IDGEN_IDENTIFIER_VERSION$;</value>
	    </param>
	    <param>
		<key>--tu_list_file</key>
		<value>$;TU_LIST_FILE$;</value>
	    </param>
	    <param>
		<key>--model_list_file</key>
		<value>$;MODEL_LIST_FILE$;</value>
	    </param>
	    <param>
		<key>--no_die_null_sequences</key>
		<value>$;NO_DIE_NULL_SEQUENCES$;</value>
	    </param>
	    <param>
		<key>--sourcename</key>
		<value>$;OUTPUT_DIRECTORY$;</value>
	    </param>
	    <param>
		<key>stdout</key>
		<value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/legacy2bsml.$;SOURCE_DATABASE$;.$;ASMBL_ID$;.$;SCHEMA_TYPE$;.stdout</value>
	    </param>
	    <param>
		<key>stderr</key>
		<value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/legacy2bsml.$;SOURCE_DATABASE$;.$;ASMBL_ID$;.$;SCHEMA_TYPE$;.stderr</value>
	    </param>
	</command>
	<command>
	    <type>RunUnixCommand</type>
	    <state>incomplete</state>
	    <name>DTD validation</name>
	    <executable>$;BIN_DIR$;/dtdValid.pl</executable>
	    <arg>-d $;BSML_DTD$; $;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;SOURCE_DATABASE$;_$;ASMBL_ID$;_assembly.$;SCHEMA_TYPE$;.bsml</arg>
	</command>
    </commandSet>
</commandSetRoot>
