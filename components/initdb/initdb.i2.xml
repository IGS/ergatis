<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation='commandSet.xsd'>
    <commandSet type="serial">
	<name>Process all OBO filesr</name>
	<command>
	    <type>RunUnixCommand</type>
	    <state>incomplete</state>
	    <name>Create temporary obo2chado output directory</name>
	    <executable>mkdir</executable>
	    <arg>-p -m 777 $;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;/cachedir</value>
        </command>
	<command>
	    <type>RunUnixCommand</type>
	    <state>incomplete</state>
	    <name>Process the OBO file</name>
	    <executable>$;BIN_DIR$;/obo2chado</executable>
	    <param>
		<key>--username</key>
		<value>chado_admin</value>
	    </param>
	    <param>
		<key>--password</key>
		<value>chado_admin99</value>
	    </param>
	    <param>
		<key>--database</key>
		<value>$;DATABASE$;</value>
	    </param>
	    <param>
		<key>--server</key>
		<value>$;SERVER$;</value>
	    </param>
	    <param>
		<key>--debug_level</key>
		<value>$;DEBUG$;</value>
	    </param>
	    <param>
		<key>--ontdoc</key>
		<value>$;I_FILE_PATH$;</value>
	    </param>
	    <param>
		<key>--log4perl</key>
		<value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;/obo2chado.log</value>
	    </param>
	    <param>
		<key>--outdir</key>
		<value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;</value>
	    </param>
	    <param>
		<key>--cache_dir</key>
		<value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;/cachedir</value>
	    </param>
	    <param>
		<key>stdout</key>
		<value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;/obo2chado.stdout</value>
	    </param>
	    <param>
		<key>stderr</key>
		<value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;/obo2chado.stderr</value>
	    </param>
        </command>
	<command>
	    <type>RunUnixCommand</type>
	    <state>incomplete</state>
	    <name>Load the OBO file contents</name>
	    <executable>$;BIN_DIR$;/chadoloader</executable>
	    <param>
		<key>--username</key>
		<value>chado_admin</value>
	    </param>
	    <param>
		<key>--password</key>
		<value>chado_admin99</value>
	    </param>
	    <param>
		<key>--database</key>
		<value>$;DATABASE$;</value>
	    </param>
	    <param>
		<key>--server</key>
		<value>$;SERVER$;</value>
	    </param>
	    <param>
		<key>--debug_level</key>
		<value>5</value>
	    </param>
	    <param>
		<key>--abort</key>
		<value>1</value>
	    </param>
	    <param>
		<key>--bcpmode</key>
		<value>in</value>
	    </param>
	    <param>
		<key>--batchsize</key>
		<value>3000</value>
	    </param>
	    <param>
		<key>--logfile</key>
		<value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;/chadoloader.log</value>
	    </param>
	    <param>
		<key>--directory</key>
		<value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;</value>
	    </param>
	    <param>
		<key>stdout</key>
		<value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;/chadoloader.stdout</value>
	    </param>
	    <param>
		<key>stderr</key>
		<value>$;TMP_DIR$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;/chadoloader.stderr</value>
	    </param>
        </command>
    </commandSet>
</commandSetRoot>
