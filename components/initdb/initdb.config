[parameters initdb]
$;DATABASE$; = 
$;SERVER$; =
$;RDBMS$; = sybase
 
$;BATCHSIZE$; = 30000

[input initdb]
$;OBO_FILES$;=$;DOCS_DIR$;/relationship.obo,$;DOCS_DIR$;/relationship.aux.obo,$;DOCS_DIR$;/annotation_attributes.obo,$;DOCS_DIR$;/annotation_flags.obo,$;DOCS_DIR$;/TIGR_roles.obo,$;DOCS_DIR$;/component.obo,$;DOCS_DIR$;/ec.obo,$;DOCS_DIR$;/evidence_code.obo,$;DOCS_DIR$;/gff3_attributes.obo,$;DOCS_DIR$;/go.obo,$;DOCS_DIR$;/go_properties.obo,$;DOCS_DIR$;/output.obo,$;DOCS_DIR$;/so.obo,$;DOCS_DIR$;/so.aux.obo,$;DOCS_DIR$;/ard.obo

[output initdb]
