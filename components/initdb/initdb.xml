<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"			xsi:schemaLocation='commandSet.xsd'>
    <commandSet type="serial">
	<state>incomplete</state>
	<name>initdb workflow</name>
	<!--Preprocessing-->
	<command>
	    <type>RunUnixCommand</type>
	    <name>create output directory</name>
	    <state>incomplete</state>
	    <executable>mkdir</executable>
	    <arg>-p -m 777 $;OUTPUT_DIRECTORY$;</arg>
	</command>
	<command>
	    <type>RunUnixCommand</type>
	    <name>create temp directory</name>
	    <state>incomplete</state>
	    <executable>mkdir</executable>
	    <arg>-p -m 777 $;TMP_DIR$;</arg>
	</command>
	<!--Iterator1 for revoking all permissions -->
	<command>
	    <type>RunUnixCommand</type>
	    <name>Create revoke permissions DDL list</name>
	    <state>incomplete</state>
	    <executable>$;BIN_DIR$;/create_file_iterator_lists.pl</executable>
            <param>
		<key>stdin</key>
		<value>$;DOCS_DIR$;/revoke_permissions.ddl</value>
	    </param>
	    <param>
		<key>stdout</key>    
		<value>$;TMP_DIR$;/revoke_permissions.list</value>
	    </param>
	</command>
	<INCLUDE file="$;DOCS_DIR$;/iterator_template.xml" keys="$;ITERATOR_NAME$;=ITERATOR1,$;ITERATOR_XML$;=ITERATOR1_XML,$;ITERATOR_LIST$;=$;TMP_DIR$;/revoke_permissions.list"/> 
	<!--Iterator2 for dropping all chado tables -->
	<command>
	    <type>RunUnixCommand</type>
	    <name>Create drop chado tables DDL list</name>
	    <state>incomplete</state>
	    <executable>$;BIN_DIR$;/create_file_iterator_lists.pl</executable>
            <param>
		<key>stdin</key>
		<value>$;DOCS_DIR$;/drop_chado_tables.ddl</value>
	    </param>
	    <param>
		<key>stdout</key>    
		<value>$;TMP_DIR$;/drop_chado_tables.list</value>
	    </param>
	</command>
	<INCLUDE file="$;DOCS_DIR$;/iterator_template.xml" keys="$;ITERATOR_NAME$;=ITERATOR1,$;ITERATOR_XML$;=ITERATOR1_XML,$;ITERATOR_LIST$;=$;TMP_DIR$;/drop_chado_tables.list"/>
	<!--Iterator3 for dropping all chado-mart tables -->
	<command>
	    <type>RunUnixCommand</type>
	    <name>Create drop chado-mart table DDL list</name>
	    <state>incomplete</state>
	    <executable>$;BIN_DIR$;/create_file_iterator_lists.pl</executable>
            <param>
		<key>stdin</key>
		<value>$;DOCS_DIR$;/drop_chado_mart_tables.ddl</value>
	    </param>
	    <param>
		<key>stdout</key>    
		<value>$;TMP_DIR$;/drop_chado_mart_tables.list</value>
	    </param>
	</command>
	<INCLUDE file="$;DOCS_DIR$;/iterator_template.xml" keys="$;ITERATOR_NAME$;=ITERATOR1,$;ITERATOR_XML$;=ITERATOR1_XML,$;ITERATOR_LIST$;=$;TMP_DIR$;/drop_chado_mart_tables.list"/>
	<!--Iterator4 for creating all chado tables -->
	<command>
	    <type>RunUnixCommand</type>
	    <name>Create create chado table DDL list</name>
	    <state>incomplete</state>
	    <executable>$;BIN_DIR$;/create_file_iterator_lists.pl</executable>
            <param>
		<key>stdin</key>
		<value>$;DOCS_DIR$;/create_tables.ddl</value>
	    </param>
	    <param>
		<key>stdout</key>    
		<value>$;TMP_DIR$;/create_tables.list</value>
	    </param>
	</command>
	<INCLUDE file="$;DOCS_DIR$;/iterator_template.xml" keys="$;ITERATOR_NAME$;=ITERATOR1,$;ITERATOR_XML$;=ITERATOR1_XML,$;ITERATOR_LIST$;=$;TMP_DIR$;/create_tables.list"/>
	<!--Iterator5 for creating all chado-mart tables -->
	<command>
	    <type>RunUnixCommand</type>
	    <name>Create create chado-mart table DDL list</name>
	    <state>incomplete</state>
	    <executable>$;BIN_DIR$;/create_file_iterator_lists.pl</executable>
            <param>
		<key>stdin</key>
		<value>$;DOCS_DIR$;/create_chado_mart_tables.ddl</value>
	    </param>
	    <param>
		<key>stdout</key>    
		<value>$;TMP_DIR$;/create_chado_mart_tables.list</value>
	    </param>
	</command>
	<INCLUDE file="$;DOCS_DIR$;/iterator_template.xml" keys="$;ITERATOR_NAME$;=ITERATOR1,$;ITERATOR_XML$;=ITERATOR1_XML$;ITERATOR_LIST$;=$;TMP_DIR$;/create_chado_mart_tables.list"/>
	<!--Iterator6 for partitioning appropriate chado tables -->
	<command>
	    <type>RunUnixCommand</type>
	    <name>Create partition table DDL list</name>
	    <state>incomplete</state>
	    <executable>$;BIN_DIR$;/create_file_iterator_lists.pl</executable>
            <param>
		<key>stdin</key>
		<value>$;DOCS_DIR$;/partition_tables.ddl</value>
	    </param>
	    <param>
		<key>stdout</key>    
		<value>$;TMP_DIR$;/partition_tables.list</value>
	    </param>
	</command>
	<INCLUDE file="$;DOCS_DIR$;/iterator_template.xml" keys="$;ITERATOR_NAME$;=ITERATOR1,$;ITERATOR_XML$;=ITERATOR1_XML$;ITERATOR_LIST$;=$;TMP_DIR$;/partition_tables.list"/>
	<command>
	    <type>RunJavaUnixCommand</type>
	    <name>Load pub record</name>
	    <executable>$;BIN_DIR$;/chadoloader</executable>
	    <param>
		<key>--database</key>
		<value>$;DATABASE$;</value>
	    </param>
	    <param>	  
		<key>--server</key>
		<value>$;SERVER$;</value>
	    </param>
	    <param>
		<key>--username</key>
		<value>chado_admin</value>
	    </param>
	    <param>
		<key>--password</key>
		<value>chado_admin99</value>
	    </param>
	    <param>
		<key>--debug_level</key>
		<value>5</value>
	    </param>
	    <param>
		<key>--bcpmode</key>
		<value>in</value>
	    </param>
	    <param>
		<key>--batchsize</key>
		<value>3000</value>
	    </param>
	    <param>
		<key>--logfile</key>
		<value>$;WORKFLOW_REPOSITORY$;/load.pub.log</value>
	    </param>
	    <param>
		<key>--infile</key>
		<value>$;WORKFLOWDOCS_DIR$;/pub.out</value>
	    </param>
	    <param>
		<key>--abort</key>
		<value>1</value>
	    </param>
	    <param>
		<key>--infile</key>
		<value>$;WORKFLOWDOCS_DIR$;/pub.out</value>
	    </param>
	    <param>
		<key>stdout</key>
		<value>$;WORKFLOW_REPOSITORY$;/load.pub.stdout</value>
	    </param>
	    <param>
		<key>stderr</key>
		<value>$;WORKFLOW_REPOSITORY$;/load.pub.stderr</value>
	    </param>
	</command>
	<!--Iterator7 for processing and loading all OBO files -->
	<INCLUDE file="$;DOCS_DIR$;/iterator_template.xml" keys="$;ITERATOR_NAME$;=ITERATOR2,$;ITERATOR_XML$;=ITERATOR2_XML,$;ITERATOR_LIST$;=$;DOCS_DIR$;/obo.list"/>
	<!--Iterator8 for creating all indices and constraints -->
	<command>
	    <type>RunUnixCommand</type>
	    <name>Create create indices and constraints iterator list file</name>
	    <state>incomplete</state>
	    <executable>$;BIN_DIR$;/create_file_iterator_lists.pl</executable>
            <param>
		<key>stdin</key>
		<value>$;DOCS_DIR$;/create_indices.ddl</value>
	    </param>
	    <param>
		<key>stdout</key>    
		<value>$;TMP_DIR$;/create_indices.list</value>
	    </param>
	</command>
	<INCLUDE file="$;DOCS_DIR$;/iterator_template.xml" keys="$;ITERATOR_NAME$;=ITERATOR1,$;ITERATOR_XML$;=ITERATOR1_XML,$;ITERATOR_LIST$;=$;TMP_DIR$;/create_indices.list"/>
	<!--Iterator9 for granting all permissions on all chado tables -->
	<command>
	    <type>RunUnixCommand</type>
	    <name>Create iterator list for granting permissions on all chado tables</name>
	    <state>incomplete</state>
	    <executable>$;BIN_DIR$;/create_file_iterator_lists.pl</executable>
            <param>
		<key>stdin</key>
		<value>$;DOCS_DIR$;/grant_permissions.ddl</value>
	    </param>
	    <param>
		<key>stdout</key>    
		<value>$;TMP_DIR$;/grant_permissions.list</value>
	    </param>
	</command>
	<INCLUDE file="$;DOCS_DIR$;/iterator_template.xml" keys="$;ITERATOR_NAME$;=ITERATOR1,$;ITERATOR_XML$;=ITERATOR1_XML,$;ITERATOR_LIST$;=$;TMP_DIR$;/grant_permissions.list"/>
	<!--Iterator10 for granting all permissions on all chado-mart tables -->
	<command>
	    <type>RunUnixCommand</type>
	    <name>Create iterator list for granting permissions on all chado-mart tables</name>
	    <state>incomplete</state>
	    <executable>$;BIN_DIR$;/create_file_iterator_lists.pl</executable>
            <param>
		<key>stdin</key>
		<value>$;DOCS_DIR$;/grant_chado_mart_permissions.ddl</value>
	    </param>
	    <param>
		<key>stdout</key>    
		<value>$;TMP_DIR$;/grant_chado_mart_permissions.list</value>
	    </param>
	</command>
	<INCLUDE file="$;DOCS_DIR$;/iterator_template.xml" keys="$;ITERATOR_NAME$;=ITERATOR1,$;ITERATOR_XML$;=ITERATOR1_XML,$;ITERATOR_LIST$;=$;TMP_DIR$;/grant_chado_mart_permissions.list"/>
	<command>
	    <type>RunJavaUnixCommand</type>
	    <name>Store chado revision number</name>
	    <executable>$;BIN_DIR$;/store_chado_version</executable>
	    <param>
		<key>--database</key>
		<value>$;DATABASE$;</value>
	    </param>
	    <param>	  
		<key>--server</key>
		<value>$;SERVER$;</value>
	    </param>
	    <param>
		<key>--username</key>
		<value>chado_admin</value>
	    </param>
	    <param>
		<key>--password</key>
		<value>chado_admin99</value>
	    </param>
	    <param>
		<key>--debug_level</key>
		<value>$;DEBUG$;</value>
	    </param>
	    <param>
		<key>--bin_dir</key>
		<value>$;BIN_DIR$;</value>
	    </param>
	    <param>
		<key>--workflow</key>
		<value>$;WORKFLOW_REPOSITORY$;</value>
	    </param>
	    <param>
		<key>--log4perl</key>
		<value>$;WORKFLOW_REPOSITORY$;/store_chado_version.log</value>
	    </param>
	    <param>
		<key>stdout</key>
		<value>$;WORKFLOW_REPOSITORY$;/store_chado_version.stdout</value>
	    </param>
	    <param>
		<key>stderr</key>
		<value>$;WORKFLOW_REPOSITORY$;/store_chado_version.stderr</value>
	    </param>
	</command>
    </commandSet>
</commandSetRoot>
