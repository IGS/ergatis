<?xml version="1.0" encoding="UTF-8"?>

<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation='commandSet.xsd'>
    <commandSet type="serial">
        <name>mothur trim seqs compute</name>
        <command>
            <type>RunUnixCommand</type>
            <name>change directory</name>
            <state>incomplete</state>
            <executable>cd</executable>
            <arg>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;</arg>
        </command>
        <command>
            <type>RunUnixCommand</type>
            <name>trim seqs</name>
            <state>incomplete</state>
            <executable>$;MOTHUR_EXEC$;</executable>
            <arg>"trim.seqs(fasta=$;I_FILE_PATH$;, oligos=$;OLIGOS_FILE$;, minlength=$;MINIMUM_LENGTH$;, maxlength=$;MAXIMUM_LENGTH$;, qfile=$;QUALITY_FILE$; $;OTHER_OPTS$;)"</arg>
        </command>
    </commandSet>
</commandSetRoot>
