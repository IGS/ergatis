<?xml version="1.0" encoding="UTF-8"?>

<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation='commandSet.xsd'>
    <commandSet type="serial">
        <name>getorf compute</name>
        <command>
            <type>RunUnixCommand</type>
            <name>Run EMBOSS getorf</name>
			<state>incomplete</state>
			<executable>$;EMBOSS_BIN$;/getorf</executable>
			<arg>-sequence $;I_FILE_PATH$; -osdirectory2 $;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/ -osname2 $;I_FILE_BASE$; -osextension2 .fsa -table $;TABLE$; -minsize $;MIN_ORF_LENGTH$; $;OTHER_OPTS$;</arg>
			<param>
				<key>stdout</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.stdout</value>
			</param>
			<param>
				<key>stderr</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/$;I_FILE_BASE$;.stderr</value>
			</param>
        </command>
    </commandSet>
</commandSetRoot>
