<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"			xsi:schemaLocation='commandSet.xsd'>
  <commandSet type="serial">
    <name>load bcp files</name>
      <command>
	  <type>RunUnixCommand</type>
	  <name>load tables</name>
	  <state>incomplete</state>
	  <executable>$;BIN_DIR$;/flatFileToChado</executable>
	  <param>
	      <key>--username</key>
	      <value>chado_admin</value>
	  </param>
	  <param>
	      <key>--password</key>
	      <value>chado_admin99</value>
	  </param>
	  <param>
	      <key>--database</key>
	      <value>$;DATABASE$;</value>
	  </param>
	  <param>
	      <key>--server</key>
	      <value>$;SERVER$;</value>
	  </param>
	  <param>
	      <key>--bcpmode</key>
	      <value>in</value>
	  </param>
	  <param>
	      <key>--batchsize</key>
	      <value>$;BCP_BATCH_SIZE$;</value>
	  </param>
	  <param>
	      <key>--infile</key>
	      <value>$;I_BCPFILE$;</value>
	  </param>
	  <param>
	      <key>--abort</key>
	      <value>0</value>
	  </param>
	  <param>
	      <key>--debug_level</key>
	      <value>$;DEBUG$;</value>
	  </param>
	  <param>
	      <key>--testmode</key>
	      <value>$;SKIP_LOAD$;</value>
	  </param>
	  <param>
	      <key>--ignore_empty_bcp</key>
	      <value>1</value>
	  </param>
	  <param>
	      <key>--gzip_bcp </key>
	      <value> $;GZIP_BCP$;</value>
	  </param>
	  <param>
	      <key>--logfile</key>
	      <value>$;WORKFLOW_REPOSITORY$;/chadoloader.pl.log</value>
	  </param>
	  <param>
	      <key>stdout</key>
	      <value>$;WORKFLOW_REPOSITORY$;/chadoloader.pl.stdout</value>
	  </param>
	  <param>
	      <key>stderr</key>
	      <value>$;WORKFLOW_REPOSITORY$;/chadoloader.pl.stderr</value></param>
      </command>
  </commandSet>
</commandSetRoot>
