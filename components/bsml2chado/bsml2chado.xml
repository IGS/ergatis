<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"			xsi:schemaLocation='commandSet.xsd'>
    <commandSet type="serial">
	<state>incomplete</state>
	<name>bsml2chado workflow</name>
	<!--Preprocessing-->
	<command>
	    <type>RunUnixCommand</type>
	    <name>create output directory</name>
	    <state>incomplete</state>
	    <executable>mkdir</executable>
	    <arg>-p -m 777 $;OUTPUT_DIRECTORY$;</arg>
	</command>
	<command>
	    <type>RunUnixCommand</type>
	    <name>create temp directory</name>
	    <state>incomplete</state>
	    <executable>mkdir</executable>
	    <arg>-p -m 777 $;TMP_DIR$;</arg>
	</command>
	<command>
	    <type>RunUnixCommand</type>
	    <name>create temp directory</name>
	    <state>incomplete</state>
	    <executable>mkdir</executable>
	    <arg>-p -m 777 $;TMP_DIR$;/cache_dir</arg>
	</command>
	<command>
	    <type>RunUnixCommand</type>
	    <name>Cache primary keys</name>
	    <executable>$;BIN_DIR$;/queries2Chado</executable>
	    <param>
		<key>--database</key>
		<value>$;DATABASE$;</value>
	    </param>
	    <param>	  
		<key>--server</key>
		<value>$;SERVER$;</value>
	    </param>
	    <param>
		<key>--username</key>
		<value>$;USERNAME$;</value>
	    </param>
	    <param>
		<key>--password</key>
		<value>$;PASSWORD$;</value>
	    </param>
	    <param>
		<key>--debug_level</key>
		<value>$;DEBUG$;</value>
	    </param>
	    <param>
		<key>--update</key>
		<value>$;UPDATE$;</value>
	    </param>
	    <param>
		<key>--type</key>
		<value>$;SEQUENCE_TYPE$;</value>
	    </param>
	    <param>
		<key>--log4perl</key>
		<value>$;WORKFLOW_REPOSITORY$;/queries2Chado.pl.log</value>
	    </param>
	    <param>
		<key>--cache_dir</key>
		<value>$;TMP_DIR$;/cache_dir</value>
	    </param>
	    <param>
		<key>stdout</key>
		<value>$;WORKFLOW_REPOSITORY$;/queries2Chado.stdout</value>
	    </param>
	    <param>
		<key>stderr</key>
		<value>$;WORKFLOW_REPOSITORY$;/queries2Chado.stderr</value>
	    </param>
	</command>
	<!--Processing-->
	<!--Iterator-->
	<INCLUDE file="$;DOCS_DIR$;/file_iterator_template.xml" keys="$;ITERATOR_NAME$;=ITERATOR1,$;ITERATOR_XML$;=ITERATOR1_XML,$;ITERATOR_TIMESTAMP$;=1"/>
	<command>
	    <type>RunUnixCommand</type>
	    <name>create_table_iterator_list</name>
	    <state>incomplete</state>
	    <executable>$;BIN_DIR$;/ddl2iterator</executable>
            <param>
		<key>stdin</key>
		<value>$;DOCS_DIR$;/create_tables.ddl</value>
	    </param>
	    <param>
		<key>stdout</key>    
		<value>$;WORKFLOW_REPOSITORY$;/collatetables.list</value>
	    </param>
	</command>
	<!--Iterator-->
	<INCLUDE file="$;DOCS_DIR$;/iterator_template.xml" keys="$;ITERATOR_NAME$;=ITERATOR2,$;ITERATOR_XML$;=ITERATOR2_XML;,$;ITERATOR_LIST$;=$;WORKFLOW_REPOSITORY$;/collatetables.list,$;NODISTRIB$;=1"/> 
	<command>
	    <type>RunUnixCommand</type>
	    <name>extract_checksums</name>
	    <state>incomplete</state>
	    <executable>$;BIN_DIR$;/extract_checksums</executable>
	    <param>
		<key>--bcp_dir</key>
		<value>$;TMP_DIR$;/checksum_bcp</value>
	    </param>
	    <param>
		<key>--bcp_extension</key>
		<value>bcp.out</value>
	    </param>
	    <param>
		<key>--output_file</key>
		<value>$;TMP_DIR$;/checksums.out</value>
	    </param>
	    <param>
		<key>stdout</key>
		<value>$;WORKFLOW_REPOSITORY$;/extract_checksums.stdout</value>
	    </param>
	    <param>
		<key>stderr</key>
		<value>$;WORKFLOW_REPOSITORY$;/extract_checksums.stderr</value>
	    </param>
	</command>
	<command>
	    <type>RunUnixCommand</type>
	    <name>sort checksums</name>
	    <state>incomplete</state>
	    <executable>sort</executable>
	    <arg>-k1,1 -k2,2n -T $TMP_DIR</arg>
	    <param>
		<key>stdin</key>
		<value>$;WORKFLOW_REPOSITORY$;/checksums.out</value>
	    </param>
	    <param>
		<key>stdout</key>
		<value>$;WORKFLOW_REPOSITORY$;/checksums.out.sorted</value>
	    </param>
	</command>
	<command>
	    <type>RunUnixCommand</type>
	    <name>assign primary keys</name>
	    <state>incomplete</state>
	    <executable>$;BIN_DIR$;/assign_pks</executable>
	    <param>
		<key>--database</key>
		<value>$;DATABASE$;</value>
	    </param>
	    <param>	  
		<key>--server</key>
		<value>$;SERVER$;</value>
	    </param>
	    <param>
		<key>--username</key>
		<value>$;USERNAME$;</value>
	    </param>
	    <param>
		<key>--password</key>
		<value>$;PASSWORD$;</value>
	    </param>
	    <param>
		<key>--debug_level</key>
		<value>$;DEBUG$;</value>
	    </param>
	    <param>
		<key>--checksum_file</key>
		<value>$;WORKFLOW_REPOSITORY$;/checksums.out.sorted</value>
	    </param>
	    <param>
		<key>--output_file</key>
		<value>$;WORKFLOW_REPOSITORY$;/checksums.out.sorted.pks</value>
	    </param>
	</command> 
	<command>
	    <type>RunUnixCommand</type>
	    <name>sort checksums by file offset</name>
	    <state>incomplete</state>
	    <executable>sort</executable>
	    <arg>-k1,1 -k2,2n -T $TMP_DIR</arg>
	    <param>
		<key>stdin</key>
		<value>$;WORKFLOW_REPOSITORY$;/checksums.out.sorted.pks</value>
	    </param>
	    <param>
		<key>stdout</key>
		<value>$;WORKFLOW_REPOSITORY$;/checksums.out.sorted.pks.offsets</value>
	    </param>
	</command>
	<command>
	    <type>RunUnixCommand</type>
	    <name>replace checksums</name>
	    <state>incomplete</state>
	    <executable>$;BIN_DIR$;/replace_checksums</executable>
	    <param>
		<key>--bcp_dir</key>
		<value>$;TMP_DIR$;</value>
	    </param>
	    <param>
		<key>--output_dir</key>
		<value>$;OUTPUT_DIRECTORY$;</value>
	    </param>
	    <param>
		<key>--bcp_extension</key>
		<value>bcp.out</value>
	    </param>
	    <param>
		<key>--checksum_file</key>
		<value>$;WORKFLOW_REPOSITORY$;/checksums.out.sorted.pks.offsets</value>
	    </param>
	    <param>
		<key>stdout</key>
		<value>$;WORKFLOW_REPOSITORY$;/replace_checksums.stdout</value>
	    </param>
	    <param>
		<key>stderr</key>
		<value>$;WORKFLOW_REPOSITORY$;/replace_checksums.stderr</value>
	    </param>
	</command>
	<command>
	    <type>RunUnixCommand</type>
	    <name>Drop indexes</name>
	    <state>incomplete</state>
	    <executable>$;BIN_DIR$;/ddl2iterator</executable>
            <param>
		<key>stdin</key>
		<value>$;DOCS_DIR$;/drop_indexes.ddl</value>
	    </param>
	    <param>
		<key>stdout</key>    
		<value>$;WORKFLOW_REPOSITORY$;/drop_indexes.list</value>
	    </param>
	</command>
	<INCLUDE file="$;DOCS_DIR$;/iterator_template.xml" keys="$;ITERATOR_NAME$;=ITERATOR3,$;ITERATOR_XML$;=ITERATOR3_XML,$;ITERATOR_LIST$;=$;WORKFLOW_REPOSITORY$;/drop_indexes.list,$;NODISTRIB$;=1"/>
	<command>
	    <type>RunUnixCommand</type>
	    <name>list bcp files</name>
	    <state>incomplete</state>
	    <executable>$;BIN_DIR$;/bcpdir2iterator</executable>
	    <param>
		<key>--bcp_directory</key>
		<value>$;OUTPUT_DIRECTORY$;</value>
	    </param>
	    <param>
		<key>--bcp_extension</key>
		<value>bcp.out</value>
	    </param>
	    <param>
		<key>--output_iterator_list</key>
		<value>$;WORKFLOW_REPOSITORY$;/bcp.out.list</value>
	    </param>
	</command>
	<INCLUDE file="$;DOCS_DIR$;/iterator_template.xml" keys="$;ITERATOR_NAME$;=ITERATOR4,$;ITERATOR_XML$;=ITERATOR4_XML,$;ITERATOR_LIST$;=$;WORKFLOW_REPOSITORY$;/bcp.out.list,$;NODISTRIB$;=1"/>
	<command>
	    <type>RunUnixCommand</type>
	    <name>Rebuild indexes</name>
	    <state>incomplete</state>
	    <executable>$;BIN_DIR$;/ddl2iterator</executable>
            <param>
		<key>stdin</key>
		<value>$;DOCS_DIR$;/create_indexes.ddl</value>
	    </param>
	    <param>
		<key>stdout</key>    
		<value>$;WORKFLOW_REPOSITORY$;/create_indexes.list</value>
	    </param>
	</command>
	<INCLUDE file="$;DOCS_DIR$;/iterator_template.xml" keys="$;ITERATOR_NAME$;=ITERATOR5,$;ITERATOR_XML$;=ITERATOR5_XML,$;ITERATOR_LIST$;=$;WORKFLOW_REPOSITORY$;/create_indexes.list,$;NODISTRIB$;=1"/>
    </commandSet>
</commandSetRoot>
