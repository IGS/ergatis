<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"			xsi:schemaLocation='commandSet.xsd'>
    <commandSet type="serial">
	<state>incomplete</state>
	<name>bsml2chado workflow</name>
	<!--Preprocessing-->
	<command>
	    <type>RunUnixCommand</type>
	    <name>create output directory</name>
	    <state>incomplete</state>
	    <executable>mkdir</executable>
	    <arg>-p -m 777 $;OUTPUT_DIRECTORY$;</arg>
	</command>
	<command>
	    <type>RunUnixCommand</type>
	    <name>create temp directory</name>
	    <state>incomplete</state>
	    <executable>mkdir</executable>
	    <arg>-p -m 777 $;TMP_DIR$;</arg>
	</command>
	<command>
	    <type>RunUnixCommand</type>
	    <name>create temp directory</name>
	    <state>incomplete</state>
	    <executable>mkdir</executable>
	    <arg>-p -m 777 $;TMP_DIR$;/cache_dir</arg>
	</command>
	<command>
	    <type>RunUnixCommand</type>
	    <name>Cache primary keys</name>
	    <executable>$;BIN_DIR$;/queries2Chado</executable>
	    <param>
		<key>--database</key>
		<value>$;DATABASE$;</value>
	    </param>
	    <param>	  
		<key>--server</key>
		<value>$;SERVER$;</value>
	    </param>
	    <param>
		<key>--username</key>
		<value>$;USERNAME$;</value>
	    </param>
	    <param>
		<key>--password</key>
		<value>$;PASSWORD$;</value>
	    </param>
	    <param>
		<key>--debug_level</key>
		<value>$;DEBUG$;</value>
	    </param>
	    <param>
		<key>--update</key>
		<value>$;UPDATE$;</value>
	    </param>
	    <param>
		<key>--type</key>
		<value>$;SEQUENCE_TYPE$;</value>
	    </param>
	    <param>
		<key>--log4perl</key>
		<value>$;WORKFLOW_REPOSITORY$;/queries2Chado.pl.log</value>
	    </param>
	    <param>
		<key>--cache_dir</key>
		<value>$;TMP_DIR$;/cache_dir</value>
	    </param>
	    <param>
		<key>stdout</key>
		<value>$;WORKFLOW_REPOSITORY$;/queries2Chado.stdout</value>
	    </param>
	    <param>
		<key>stderr</key>
		<value>$;WORKFLOW_REPOSITORY$;/queries2Chado.stderr</value>
	    </param>
	</command>
	<!--Processing-->
	<!--Iterator-->
	<INCLUDE file="$;DOCS_DIR$;/file_iterator_template.xml" keys="$;ITERATOR_NAME$;=ITERATOR1,$;ITERATOR_XML$;=ITERATOR1_XML"/>
	<command>
	    <type>RunUnixCommand</type>
	    <name>create_table_iterator_list</name>
	    <state>incomplete</state>
	    <executable>$;BIN_DIR$;/parse_table_names.pl</executable>
            <param>
		<key>stdin</key>
		<value>$;DOCS_DIR$;/create_tables.ddl</value>
	    </param>
	    <param>
		<key>stdout</key>    
		<value>$;TMP_DIR$;/tables.list</value>
	    </param>
	</command>
	<!--Iterator-->
	<INCLUDE file="$;DOCS_DIR$;/iterator_template.xml" keys="$;ITERATOR_NAME$;=ITERATOR2,$;ITERATOR_XML$;=ITERATOR2_XML;$;ITERATOR_LIST$;=$;TMP_DIR$;/tables.list"/> 
	<command>
	    <type>RunUnixCommand</type>
	    <name>extract_checksums</name>
	    <state>incomplete</state>
	    <executable>$;BIN_DIR$;/extract_checksums</executable>
	    <param>
		<key>--bcp_dir</key>
		<value>$;TMP_DIR$;/checksum_bcp</value>
	    </param>
	    <param>
		<key>--bcp_extension</key>
		<value>bcp.out</value>
	    </param>
	    <param>
		<key>--output_file</key>
		<value>$;TMP_DIR$;/checksums.out</value>
	    </param>
	    <param>
		<key>stdout</key>
		<value>$;WORKFLOW_REPOSITORY$;/extract_checksums.stdout</value>
	    </param>
	    <param>
		<key>stderr</key>
		<value>$;WORKFLOW_REPOSITORY$;/extract_checksums.stderr</value>
	    </param>
	</command>
	<command>
	    <type>RunUnixCommand</type>
	    <name>sort checksums</name>
	    <state>incomplete</state>
	    <executable>sort</executable>
	    <arg>-k1,1 -k2,2n -T $TMP_DIR</arg>
	    <param>
		<key>stdin</key>
		<value>$;WORKFLOW_REPOSITORY$;/checksums.out</value>
	    </param>
	    <param>
		<key>stdout</key>
		<value>$;WORKFLOW_REPOSITORY$;/checksums.out.sorted</value>
	    </param>
	</command>
	<command>
	    <type>RunUnixCommand</type>
	    <name>assign primary keys</name>
	    <state>incomplete</state>
	    <executable>$;BIN_DIR$;/assign_pks</executable>
	    <param>
		<key>--database</key>
		<value>$;DATABASE$;</value>
	    </param>
	    <param>	  
		<key>--server</key>
		<value>$;SERVER$;</value>
	    </param>
	    <param>
		<key>--username</key>
		<value>$;USERNAME$;</value>
	    </param>
	    <param>
		<key>--password</key>
		<value>$;PASSWORD$;</value>
	    </param>
	    <param>
		<key>--debug_level</key>
		<value>$;DEBUG$;</value>
	    </param>
	    <param>
		<key>--checksum_file</key>
		<value>$;WORKFLOW_REPOSITORY$;/checksums.out.sorted</value>
	    </param>
	    <param>
		<key>--output_file</key>
		<value>$;WORKFLOW_REPOSITORY$;/checksums.out.sorted.pks</value>
	    </param>
	</command> 
	<command>
	    <type>RunUnixCommand</type>
	    <name>sort checksums by file offset</name>
	    <state>incomplete</state>
	    <executable>sort</executable>
	    <arg>-k1,1 -k2,2n -T $TMP_DIR</arg>
	    <param>
		<key>stdin</key>
		<value>$;WORKFLOW_REPOSITORY$;/checksums.out.sorted.pks</value>
	    </param>
	    <param>
		<key>stdout</key>
		<value>$;WORKFLOW_REPOSITORY$;/checksums.out.sorted.pks.offsets</value>
	    </param>
	</command>
	<command>
	    <type>RunUnixCommand</type>
	    <name>replace checksums</name>
	    <state>incomplete</state>
	    <executable>$;BIN_DIR$;/replace_checksums</executable>
	    <param>
		<key>--bcp_dir</key>
		<value>$;TMP_DIR$;</value>
	    </param>
	    <param>
		<key>--output_dir</key>
		<value>$;OUTPUT_DIRECTORY$;</value>
	    </param>
	    <param>
		<key>--bcp_extension</key>
		<value>bcp.out</value>
	    </param>
	    <param>
		<key>--checksum_file</key>
		<value>$;WORKFLOW_REPOSITORY$;/checksums.out.sorted.pks.offsets</value>
	    </param>
	    <param>
		<key>stdout</key>
		<value>$;WORKFLOW_REPOSITORY$;/replace_checksums.stdout</value>
	    </param>
	    <param>
		<key>stderr</key>
		<value>$;WORKFLOW_REPOSITORY$;/replace_checksums.stderr</value>
	    </param>
	</command>
	<command>
	    <type>RunUnixCommand</type>
	    <name>drop indices</name>
	    <state>incomplete</state>
	    <executable>$;BIN_DIR$;/sql2Chado</executable>
	    <param><key>--username</key><value>chado_admin</value></param>
	    <param><key>--password</key><value>chado_admin99</value></param>
	    <param><key>--database</key><value>$;DATABASE$;</value></param>
	    <param><key>--server</key><value>$;SERVER$;</value></param>
	    <param><key>--action</key><value>dropindexes</value></param>
	    <param><key>--testmode</key><value>$;SKIP_LOAD$;</value></param>
	    <param><key>--indexmanip</key><value>$;REFRESH_INDEXES$;</value></param>
	    <param><key>--debug_level</key><value>$;DEBUG$;</value></param>
	    <param><key>--file</key><value>$;DOCS_DIR$;/drop_indexes.ddl</value></param>
	    <param><key>--log4perl</key><value>$;WORKFLOW_REPOSITORY$;/dropindex.log</value></param>
	    <param><key>stdout</key><value>$;WORKFLOW_REPOSITORY$;/dropindex.stdout</value></param>
	    <param><key>stderr</key><value>$;WORKFLOW_REPOSITORY$;/dropindex.stderr</value></param>
	</command>
	<command>
	    <type>RunUnixCommand</type>
	    <name>load tables</name>
	    <state>incomplete</state>
	    <executable>$;BIN_DIR$;/chadoloader</executable>
	    <param><key>--username</key><value>chado_admin
		<param><key>--password</key><value>chado_admin99</value></param>
		<param><key>--database</key><value>$;DATABASE$;</value></param>
		<param><key>--server</key><value>$;SERVER$;</value></param>
		<param><key>--bcpmode</key><value>in</value></param>
		<param><key>--batchsize</key><value>$;BCP_BATCH_SIZE$;</value></param>
		<param><key>--directory</key><value>$;OUTPUT_DIRECTORY$;</value></param>
		<param><key>--abort</key><value>0</value></param>
		<param><key>--debug_level</key><value>$;DEBUG$;</value></param>
		<param><key>--testmode</key><value>$;SKIP_LOAD$;</value></param>
		<param><key>--ignore_empty_bcp</key><value>1</value></param>
		<param><key>--gzip_bcp </key><value> $;GZIP_BCP$;</value></param>
		<param><key>--logfile</key><value>$;WORKFLOW_REPOSITORY$;/chadoloader.pl.log</value></param>
		<param><key>stdout</key><value>$;WORKFLOW_REPOSITORY$;/chadoloader.pl.stdout</value></param>
		<param><key>stderr</key><value>$;WORKFLOW_REPOSITORY$;/chadoloader.pl.stderr</value></param>
</command>
	<command>
	    <type>RunUnixCommand</type>
	    <name>rebuild indices</name>
	    <state>incomplete</state>
	    <executable>$;BIN_DIR$;/sql2Chado</executable>
	    <param><key>--username</key><value>chado_admin</value></param>
	    <param><key>--password</key><value>chado_admin99</value></param>
	    <param><key>--database</key><value>$;DATABASE$;</value></param>
	    <param><key>--server</key><value>$;SERVER$;</value></param>
	    <param><key>--action</key><value>createindexes</value></param>
	    <param><key>--testmode</key><value>$;SKIP_LOAD$;</value></param>
	    <param><key>--indexmanip</key><value>$;REFRESH_INDEXES$;</value></param>
	    <param><key>--debug_level</key><value>$;DEBUG$;</value></param>
	    <param><key>--file</key><value>$;DOCS_DIR$;/create_indexes.ddl</value></param>
	    <param><key>--log4perl</key><value>$;WORKFLOW_REPOSITORY$;/createindex.log</value></param>
	    <param><key>stdout</key><value>$;WORKFLOW_REPOSITORY$;/createindex.stdout</value></param>
	    <param><key>stderr</key><value>$;WORKFLOW_REPOSITORY$;/createindex.stderr</value></param>
	</command>

    </commandSet>
</commandSetRoot>
