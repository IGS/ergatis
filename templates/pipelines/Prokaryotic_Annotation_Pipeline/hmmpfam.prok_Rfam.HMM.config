[interface]
classification = alignment / model

[parameters]
$;OTHER_OPTS$;=-n
$;COMPRESS_BSML_OUTPUT$;=0

[input]
$;INPUT_FILE_LIST$;=$;REPOSITORY_ROOT$;/output_repository/split_multifasta/$;PIPELINEID$;_default/split_multifasta.fsa.list
$;INPUT_FILE$;=
$;INPUT_DIRECTORY$;=
;; the following is only used when iterating over an INPUT_DIRECTORY
$;INPUT_EXTENSION$;=fsa
$;DATABASE_PATH$;=/usr/local/db/RFAM/HMMs/prok_Rfam.HMM
