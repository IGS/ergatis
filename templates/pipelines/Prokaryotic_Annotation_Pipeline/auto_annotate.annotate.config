$;PANDA_HEADER_FILE$;=/usr/local/devel/ANNOTATION/kgalens/data_dbs/parsedPanda_header
;; role id information BDB
$;ROLE_INFO_DB$;=/usr/local/annotation/MOORE/data_dbs/guessRoleData.db
;; Cutoffs for ber analysis 
$;PERCENT_ID_CUTOFF$;=35
$;LENGTH_CUTOFF$;=80
$;COMPRESS_BSML_OUTPUT$;=0

[input]
