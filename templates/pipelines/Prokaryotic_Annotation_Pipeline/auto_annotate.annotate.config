[interface]
classification = functional prediction

[parameters]
;; directory or list file of hmm bsml files
$;HMM_ANALYSIS$;=$;REPOSITORY_ROOT$;/output_repository/hmmpfam/$;PIPELINEID$;_pre_overlap_analysis/hmmpfam.bsml.list,$;REPOSITORY_ROOT$;/output_repository/hmmpfam/$;PIPELINEID$;_post_overlap_analysis/hmmpfam.bsml.list
;; directory or list file of ber bsml files
$;BER_ANALYSIS$;=$;REPOSITORY_ROOT$;/output_repository/ber/$;PIPELINEID$;_pre_overlap_analysis/ber.bsml.list,$;REPOSITORY_ROOT$;/output_repository/ber/$;PIPELINEID$;_post_overlap_analysis/ber.bsml.list
;; MLDBM dump of hmm info 
$;HMM_INFO_DB$;=/usr/local/annotation/MOORE/data_dbs/hmmInfo.db
;; Split_DB_File basename of panda header offset
$;PANDA_HEADER_OFFSETS$;=/usr/local/devel/ANNOTATION/kgalens/data_dbs/parsedPanda
