<?xml version="1.0" encoding="UTF-8"?>

<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
				xsi:schemaLocation='commandSet.xsd'>
    <commandSet type="serial">
        <name>bsml2chado workflow</name>
        <configMapId>empty</configMapId>
         <command>    
            <name>RunUnixCommand</name>
            <configMapId>remove_scratch</configMapId>
        </command>
         <command>    
            <name>RunUnixCommand</name>
            <configMapId>create_scratch1</configMapId>
        </command>
         <command>    
            <name>RunUnixCommand</name>
            <configMapId>create_scratch2</configMapId>
        </command>

        <commandSet type="parallel">
            <name>Run bsml to chado subflow</name>
                <configMapId>empty</configMapId>
                   <commandSet type="serial">
                       <name>Iterative Workflow</name>
                       <configMapId>subflow</configMapId>
                       <config>
                       <param>
                           <key>template</key>
                           <value>bsml2chado-instance_template.xml</value>
                       </param>
                       </config>
                   </commandSet>
       </commandSet>
        <command>    
            <name>RunUnixCommand</name>
            <configMapId>generate_directory_list</configMapId>
        </command>
        <command>    
            <name>RunUnixCommand</name>
            <configMapId>replace_placeholders</configMapId>
        </command>
        <command>    
            <name>RunUnixCommand</name>
            <configMapId>validate_feature</configMapId>
        </command>
        <command>    
            <name>RunUnixCommand</name>
            <configMapId>validate_featureloc</configMapId>
        </command>
        <command>    
            <name>RunUnixCommand</name>
            <configMapId>validate_organism</configMapId>
        </command>
        <command>    
            <name>RunUnixCommand</name>
            <configMapId>validate_analysisprop</configMapId>
        </command>
        <command>    
            <name>RunUnixCommand</name>
            <configMapId>validate_analysis</configMapId>
        </command>
        <command>    
            <name>RunUnixCommand</name>
            <configMapId>validate_analysisfeature</configMapId>
        </command>
        <command>    
            <name>RunUnixCommand</name>
            <configMapId>drop_indexes</configMapId>
        </command>
        <command>    
            <name>RunUnixCommand</name>
            <configMapId>load_sybase</configMapId>
        </command>
        <command>    
            <name>RunUnixCommand</name>
            <configMapId>rebuild_indexes</configMapId>
        </command>
        <command>    
            <name>RunUnixCommand</name>
            <configMapId>confirm_database_loaded</configMapId>
        </command>
        <command>    
            <name>RunUnixCommand</name>
            <configMapId>change_log_permissions</configMapId>
        </command>
    </commandSet>
</commandSetRoot>
