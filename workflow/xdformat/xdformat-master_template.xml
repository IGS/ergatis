<?xml version="1.0" encoding="UTF-8"?>

<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
				xsi:schemaLocation='commandSet.xsd'>
    <commandSet type="serial">
      <name>xdformat workflow</name>
      <configMapId>empty</configMapId>
<!--Preprocessing-->
    	<command>
          <type>RunJavaUnixCommand</type>
          <name>create scratch directory</name>
          <configMapId>create_compute_scratch</configMapId>
        </command>
    	<command>
          <type>RunJavaUnixCommand</type>
          <name>create output directory</name>
          <configMapId>create_compute_output</configMapId>
        </command>
<!--Processing-->
    	<command>
          <type>RunJavaUnixCommand</type>
          <name>xdformat</name>
          <configMapId>xdformat</configMapId>
        </command>
<!--Postprocessing-->
     </commandSet>
</commandSetRoot>
